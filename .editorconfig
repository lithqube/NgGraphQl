# Editor configuration, see http://editorconfig.org
root = true

[*]
end_of_line = lf
charset = utf-8
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 120

[*.{properties,yaml,yml}]
max_line_length = off

[*.java]
wildcard_import_limit = 999
indent_size = 4

[*.js]
quote_type = auto
